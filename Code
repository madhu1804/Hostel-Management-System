Relation Student:

CREATE TABLE STUDENT (STUDENT_ID CHAR (9), NAME VARCHAR (18), F_NAME
VARCHAR (9), DEPARTMENT VARCHAR (18), CELL_NO NUMBER (10), ROOM_ID
CHAR (4), AGE NUMBER (2), DOB DATE, BHOSTEL_ID NUMBER (4), PRIMARY
KEY(STUDENT_ID));

