Relation Furniture:
CREATE TABLE FURNITURE (FURNITURE_ID VARCHAR (4), BED
NUMBER (1), STUDY_TABLE NUMBER (1), STUDY_CHAIR NUMBER (1), PRIMARY
KEY(FURNITURE_ID));
