NAIR','PRAJITH','EEE',9764318520,'F140',20,'01-APR-1997',1234);
INSERT INTO STUDENT VALUES ('16BME0177','SOURAV
SUDHI','SOURAV','MECH',7946138205,'M210',19,'25-DEC-1998',1423);

